tellme <- function(x) {
p1 <- paste("Type of", x, " is",typeof(x),sep=" ")
print(p1)
p2 <- paste("Class of", x, "is", class(x), sep=" ")
print(p2)
p3 <- paste("String rep of ",x," is", str(x), sep=" ")
print(p3)
p4 <- paste("Names for ", x, "is", names(x), sep=" ")
print(p4)
invisible()
}
